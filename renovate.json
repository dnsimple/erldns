{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "hex"
      ],
      "matchUpdateTypes": [
        "patch",
        "pin",
        "digest"
      ],
      "addLabels": [
        "skip-changelog"
      ]
    }
  ]
}
